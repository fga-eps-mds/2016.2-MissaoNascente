<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jbbmobile">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.RegisterScreenActivity" android:screenOrientation="portrait"/>
        <activity android:name=".view.LoginScreenActivity"/>
        <activity android:name=".view.StartScreenActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.MainScreenActivity" android:screenOrientation="portrait" />
        <activity android:name=".view.PreferenceScreenActivity" android:screenOrientation="portrait" />
        <activity android:name=".view.ElementScreenActivity" android:screenOrientation="portrait"/>
        <activity android:name=".view.AlmanacScreenActivity" android:screenOrientation="portrait"/>
    </application>

</manifest>